{
    "overrides": [
        {
            "files": ["lib/**/index.js"],
            "rules": {
                "sort-keys-fix/sort-keys-fix": "warn"
            }
        },
        {
            "files": ["index.js", "lib/**/*.js", "util/**/*.js"],
            "parserOptions": {
                "ecmaVersion": 2021
            },
            "extends": [
                "eslint:recommended",
                "plugin:node/recommended"
            ],
            "rules": {
                "no-process-exit": "off",
                "node/no-process-exit": "off"
            }
        }
    ]
}
